.popup.popup-open-form
    .popup__inner
        //button close
        .popup-services-0__close.popup-close
            +setSvg({name: 'close'})
        //form
        form.popup__form.form-block.form-submit
            .form-block__header
                .popup__form-title
                    +formTitle({text: 'Оставьте заявку <br> и получите рассчет стоимости в течении 5 минут'})
            .form-block__body
                .form-block__row
                    .form-block__col
                        +input-name({placeholder: 'Как вас зовут?'})
                    .form-block__col
                        +input-tel({placeholder: 'Ваш Email'})
                    .form-block__col
                        +input-submit({value: 'Оставить заявку'})
